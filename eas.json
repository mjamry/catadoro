{
  "cli": {
    "version": ">= 6.0.0"
  },
  "build": {
    "preview":{
      "android": {
        "buildType": "apk"
      },
      "channel": "preview",
      "env": {
        "EXPO_PUBLIC_BUILD_TYPE": "DEV"
      }
    },
    "development": {
      "android": {
        "buildType": "apk"
      },
      "channel": "development",
      "env": {
        "EXPO_PUBLIC_BUILD_TYPE": "DEV"
      }
    },
    "uat": {
      "android": {
        "buildType": "apk"
      },
      "channel": "uat",
      "env": {
        "EXPO_PUBLIC_BUILD_TYPE": "TEST"
      }
    },
    "production": {
      "android": {
        "buildType": "apk"
      },
      "channel": "production",
      "env": {
        "EXPO_PUBLIC_BUILD_TYPE": "PROD"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
